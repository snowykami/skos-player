{
  "name": "skos-player",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/electron-main.js",
  "build": {
    "extends": "./electron-builder.json"
  },
  "scripts": {
    "dev:fe": "vite",
    "build:fe": "tsc -b && vite build",
    "dev:electron": "VITE_DEV_SERVER_URL=http://127.0.0.1:5173/ pnpm -s electron:run",
    "electron:run": "pnpm -s electron:compile && node ./node_modules/electron/cli.js ./dist-electron/electron-main.js",
    "electron:compile": "tsc -p tsconfig.electron.json",
    "build:electron": "pnpm -s build:fe && pnpm -s electron:compile && pnpm -s electron:build",
    "electron:build": "electron-builder",
    "dev": "pnpm -s dev:electron",
    "build": "pnpm -s build:electron",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@snowykami/use-async-task": "^0.0.3",
    "field-conv": "^1.0.9",
    "lrc-file-parser": "^2.4.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}